{
  "name": "RefileRequestService",
  "version": "0.1.0",
  "description": "Lambda for the NYPL Refile Request API Service",
  "main": "index.js",
  "scripts": {
    "pretest": "phplint src/*.php src/**/*.php src/**/**/*.php",
    "build-node-lambda-env": "cp ./sample/.env.sample ./.env",
    "build-local-config": "mkdir -p config && cp ./sample/deployment.env.sample ./config/local.env",
    "build-development-config": "mkdir -p config && cp ./sample/deployment.env.sample ./config/var_development.env",
    "build-production-config": "mkdir -p config && cp ./sample/deployment.env.sample ./config/var_production.env",
    "build-development-sources": "cp ./sample/sample_event_sources.json ./config/event_sources_development.json",
    "build-production-sources": "cp ./sample/sample_event_sources.json ./config/event_sources_production.json",
    "deploy-development": "./node_modules/.bin/node-lambda deploy -e development -f config/var_development.env -S config/event_sources_development.json -b {subnet} -g {security-groups} -P {profile}",
    "deploy-production": "./node_modules/.bin/node-lambda deploy -e production -f config/var_production.env -S config/event_sources_production.json -b {subnet} -g {security-groups} -P {profile}",
    "local-run": "./node_modules/.bin/node-lambda run -f ./config/local.env -j ./sample/sample_event.json"
  },
  "repository": {},
  "keywords": [],
  "dependencies": {
    "http-string-parser": "^0.0.5"
  },
  "devDependencies": {
    "node-lambda": "0.11.4",
    "phplint": "~1.0.0"
  }
}
